{
  "name": "SchmacktendApp",
  "private": true,
  "workspaces": [
    "platforms/*",
    "shared/*"
  ],
  "devDependencies": {
    "@types/react": "18.2.39",
    "@types/react-native": "0.72.7",
    "eslint": "8.54.0",
    "turbo": "1.10.16",
    "typescript": "5.3.2"
  },
  "scripts": {
    "native:ios": "cd platforms/expo && npx expo run:ios",
    "native:android": "cd platforms/expo && npx expo run:android",
    "web": "cd platforms/next && yarn next",
    "build": "cd platforms/next && yarn build",
    "build-start": "cd platforms/next && yarn start"
  },
  "packageManager": "yarn@3.4.1",
  "dependencies": {
    "@tamagui/config": "latest",
    "chess.js": "1.0.0-beta.6",
    "react-native-chessboard": "0.1.2",
    "solito": "4.1.3",
    "tamagui": "latest"
  }
}
